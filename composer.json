{
    "name": "unisharp/laravel-eloquent-join",
    "description": "This package introduces the join magic for eloquent models and relations.",
    "keywords": [
        "laravel join",
        "laravel eloquent join",
        "laravel sort join",
        "laravel where join",
        "laravel join relation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Filip Horvat",
            "email": "filip.horvat@am2studio.hr",
            "homepage": "http://am2studio.hr",
            "role": "Developer"
        },
        {
            "name": "UniSharp Ltd.",
            "email": "opensource@unisharp.com"
        }
    ],
    "require": {
        "illuminate/database": "5.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.*",
        "friendsofphp/php-cs-fixer" : "2.*"
    },
    "autoload": {
        "psr-4": {
            "UniSharp\\Laravel\\EloquentJoin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UniSharp\\Laravel\\EloquentJoin\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
